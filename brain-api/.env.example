# Node Environment
NODE_ENV=production
PORT=3000

# Supabase (Server-Side Only - NEVER expose these to frontend)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database (if using PostgresAdapter instead of DevInMemoryAdapter)
# DATABASE_URL=postgresql://user:password@host:5432/database

# Google Ads OAuth
GOOGLE_DEVELOPER_TOKEN=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=https://your-app.fly.dev/oauth/google/callback

# TikTok Ads OAuth
TIKTOK_APP_ID=
TIKTOK_APP_SECRET=
TIKTOK_REDIRECT_URI=https://your-app.fly.dev/oauth/tiktok/callback

# Snapchat Ads OAuth
SNAPCHAT_CLIENT_ID=
SNAPCHAT_CLIENT_SECRET=
SNAPCHAT_REDIRECT_URI=https://your-app.fly.dev/oauth/snapchat/callback

# Token Encryption (32-byte hex string, generate with: openssl rand -hex 32)
OAUTH_ENCRYPTION_KEY=
